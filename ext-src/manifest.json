{
  "manifest_version": 2,

  "name": "One-click Selor Login",
  "description": "This extension is an autolog-on for oSoc students who need to login to the selor guest network.",
  "version": "0.1.2",
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "https://1.1.1.1/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
